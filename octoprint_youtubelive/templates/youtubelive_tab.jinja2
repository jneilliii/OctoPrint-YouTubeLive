<div id="youtubelive_wrapper" class="row-fluid">
		<iframe data-bind="attr: {src: 'https://www.youtube.com/embed/live_stream?channel=' + settingsViewModel.settings.plugins.youtubelive.channel_id()}" frameborder="0" allowfullscreen></iframe>
</div>
<div class="control-group row-fluid">
	<label class="control-label">{{ _('Go Live') }}</label>
	<div class="controls">
		<button class="btn btn-danger" data-bind="click: startStream"><i class="icon icon-play"></i>Go Live</button>
	</div>
</div>
